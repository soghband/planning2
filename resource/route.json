{
    "": {
        "controller":"index"
    },
    "login": {
        "controller": "login"
    },
    "actionLogin": {
        "controller": "actionLogin"
    },
    "actionLogout": {
        "controller": "actionLogout"
    },
    "addTask": {
        "controller": "addTask"
    },
    "actionAdd": {
        "controller": "actionAdd"
    },
    "editTask/{tno}/{spno}": {
        "controller": "editTask",
        "url_filter":{
            "{tno}": "^[0-9]*$",
            "{spno}": "^[0-9]*$"
        }
    },
    "quickAction": {
        "controller": "quickAction",
        "param_filter": {
            "taskNo":"^[0-9]*$",
            "sprintId":"^[0-9]*$",
            "type":"(startTask|endTask)",
            "time":"^[0-9.]*$"
        }
    }
}